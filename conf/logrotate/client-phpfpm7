S_PATH_LOG/php7/*.emerg
S_PATH_LOG/php7/*.err
S_PATH_LOG/php7/*.warning
S_PATH_LOG/php7/*.info
S_PATH_LOG/php7/*.notice {
	daily
	rotate 2
	missingok
	compress
	delaycompress
	notifempty
	sharedscripts
	postrotate
		/etc/init.d/rsyslog --quiet --ifstarted reload > /dev/null 2>/dev/null || true
	endscript
}